{
  "name": "prolic/fpp",
  "description": "Functional PHP Preprocessor",
  "authors": [
    {
      "name": "Sascha-Oliver Prolic",
      "email": "saschaprolic@googlemail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "phunkie/phunkie": "^0.10.2"
  },
  "require-dev": {
    "mikey179/vfsStream": "^1.6.5",
    "phpunit/phpunit": "^7.0",
    "prooph/php-cs-fixer-config": "^0.2.1"
  },
  "autoload": {
    "psr-4": {
      "Fpp\\": "src/"
    },
    "files": [
      "src/dump.php",
      "src/parse.php",
      "src/replace.php",
      "src/scan.php",
      "src/templates.php"
    ]
  }
}
